      "TextTable": [
        {
          "Id": {
            "IdString": "0",
            "FallbackId": "None",
            "IdNumber": 0
          },
          "Text": "子どもはどうだった？\r\nまだ見てないのか？　早く行け！",
          "MetaData": [
            {
              "Key": "speaker_id",
              "Value": "Npc_Urugarm"
            },
            {
              "Key": "listener_id",
              "Value": "Player"
