              "Key": "speaker_id",
              "Value": "Npc_Guido"
            },
            {
              "Key": "listener_id",
              "Value": "Player"
            }
          ],
          "Version": {
            "Versions": []
          }
        },
        {
          "Id": {
            "IdString": "67",
