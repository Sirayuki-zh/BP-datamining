            {
              "Key": "speaker_id",
              "Value": "Npc_Guido"
            },
            {
              "Key": "listener_id",
              "Value": "Npc_Cindy"
            }
          ],
          "Version": {
            "Versions": []
          }
        },
        {
          "Id": {
            "IdString": "52",
            "FallbackId": "None",
            "IdNumber": 52
          },
