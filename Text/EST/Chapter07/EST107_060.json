            {
              "Key": "speaker_id",
              "Value": "Npc_Tribesmen"
            },
            {
              "Key": "listener_id",
              "Value": "Npc_Tribesmen"
            }
          ],
          "Version": {
            "Versions": []
          }
        },
        {
          "Id": {
            "IdString": "14",
            "FallbackId": "None",
            "IdNumber": 14
          },
          "Text": "あの味は空にも登る心地であった！",
